{
  "name": "electron-app",
  "version": "1.0.0",
  "main": "main.js",
  "license": "MIT",
  "dependencies": {
    "better-sqlite3": "^12.4.1",
    "concurrently": "^9.2.1",
    "dotenv": "^17.2.3",
    "electron": "33.0.0",
    "wait-port": "^1.1.0"
  },
  "scripts": {
    "dev:next": "cd ../my-app && yarn dev",
    "dev:electron": "wait-port 3000 && electron .",
    "dev": "concurrently -k -n next,electron \"yarn dev:next\" \"yarn dev:electron\"",
    "rebuild": "electron-rebuild -f -w better-sqlite3"
  },
  "devDependencies": {
    "@electron/rebuild": "^4.0.1"
  }
}
